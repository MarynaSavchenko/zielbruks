server {
    listen 80;
    server_name staging.zielbruks.tk;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        root /home/zielbruks/staging/zielbruks;
    }

    location / {
        include my_proxy_params;
        proxy_pass http://unix:/run/gunicorn-staging.sock;
    }
}
